#include <stdio.h>
#include <math.h>
int main()
{
	float a, b;
	printf("Nhap 2 so nguyen a va b: ");
	scanf_s("%f%f", &a, &b);

	if (a == 0) {
		if (b == 0) {
			printf("Phuong trinh vo so nghiem\n");
		}
		else {
			printf("Phuong trinh vo nghiem\n");
		}
	}
	else {
		printf("Phuong trinh co nghiem:x = %f\n", -b / a);

	}

	return 0;
}
